{
    "name": "@financial-targets/accounts",
    "version": "1.0.0",
    "description": "API for application responsible for all payments",
    "main": "dist/build.js",
    "scripts": {
        "build": "npm run build:types && npm run build:js",
        "build:types": "tsc",
        "build:js": "babel ./src --out-dir ./dist --source-maps --extensions \".ts,.tsx\" --ignore ./src/**/*.test.ts",
        "clean": "rm -r ./dist",
        "dev": "nodenv -e nodemon -- --exec",
        "test": "jest --expand --detect-open-handles",
        "test:watch": "jest --expand --detect-open-handles --only-changed --watch",
        "test:watch-all": "jest --expand --detect-open-handles -- --watch",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "start": "node ./dist/server.js",
        "lint": "eslint . --ext .ts"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test && npm run lint"
        }
    },
    "nodemonConfig": {
        "exec": "babel-node ./dist/src/server.js"
    },
    "author": {
        "name": "Vinícius Rocha",
        "email": "vnc.chelsea@gmail.com",
        "url": "https://github.com/ViniciusSabino"
    },
    "contributors": [
        {
            "name": "Vinícius Rocha",
            "email": "vnc.chelsea@gmail.com",
            "url": "https://github.com/ViniciusSabino"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:ViniciusSabino/Financial_Targets.git"
    },
    "bugs": "https://github.com/ViniciusSabino/Financial_Targets/issues",
    "homepage": "https://github.com/ViniciusSabino/Financial_Targets/blob/master/README.md",
    "license": "ISC",
    "engines": {
        "node": "&gt;= 6.0.0",
        "npm": "&gt;= 3.0.0"
    },
    "dependencies": {
        "@hapi/joi": "^17.0.2",
        "chalk": "^2.4.2",
        "fastest-validator": "^0.6.17",
        "kcors": "^2.2.2",
        "koa": "^2.7.0",
        "koa-bodyparser": "^4.2.1",
        "koa-compose": "^4.1.0",
        "koa-joi-router": "^5.3.0",
        "koa-logger": "^3.2.0",
        "koa-respond": "^2.1.0",
        "moment": "^2.24.0",
        "mongoose": "^5.7.5",
        "youch": "^2.0.10"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/node": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-do-expressions": "^7.2.0",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-typescript": "^7.8.3",
        "@babel/register": "^7.0.0",
        "@types/jest": "^24.0.18",
        "@types/koa-bodyparser": "^4.3.0",
        "@types/koa-joi-router": "^5.2.3",
        "@types/koa-logger": "^3.1.1",
        "@types/koa__cors": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^2.16.0",
        "@typescript-eslint/parser": "^2.16.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.9.0",
        "dotenv": "^6.2.0",
        "dotenv-json": "^1.0.0",
        "eslint": "^5.15.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jest": "^22.17.0",
        "eslint-plugin-prettier": "^3.0.1",
        "faker": "^4.1.0",
        "husky": "^1.3.1",
        "jest": "^24.7.1",
        "mockingoose": "^2.13.1",
        "node-env-run": "^3.0.2",
        "nodemon": "^1.18.10",
        "prettier": "^1.16.4",
        "pretty-quick": "^1.10.0",
        "ts-jest": "^24.3.0",
        "typescript": "^3.7.5"
    }
}
